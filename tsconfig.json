{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./*"],
      "@root/*": ["./*"],
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@routers/*": ["src/routers/*"],
      "@utils/*": ["src/utils/*"],
      "@config": ["src/config"],
      "@pngs/*": ["src/assets/pngs/*"]
    }
  }
}
